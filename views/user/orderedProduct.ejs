<%- include('../partials/userPartials/header.ejs') %>

    <!-- BREADCRUMB AREA START -->
    <div class="ltn__breadcrumb-area text-left">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="ltn__breadcrumb-inner">
                        <h1 class="page-title">Orderded Products</h1>
                        <div class="ltn__breadcrumb-list">
                            <ul>
                                <li><a href="/"><span class="ltn__secondary-color"><i class="fas fa-home"></i></span>
                                        Home</a></li>
                                <li>Orderded Products</li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- BREADCRUMB AREA END -->

    <!-- WISHLIST AREA START -->
    <div class="liton__wishlist-area mb-105">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="shoping-cart-inner">
                        <div class="shoping-cart-table table-responsive">
                            <table class="table">
                                <!-- <thead>
                                    <th class="cart-product-image">Image</th>
                                    <th class="cart-product-info">Name</th>
                                    <th class="cart-product-subtotal">Price</th>
                                    <th class="cart-product-quantity">Quantity</th>
                                </thead> -->
                                <tbody>
                                    <% products.forEach(product=> {%>
                                    <tr>
                                            <td class="cart-product-image">
                                                <a href="/productDetails?id=<%=product.productId._id %>"><img
                                                        src="/adminAsset/productImages/<%=product.productId.images[product.productId.images.length-1] %>"
                                                        alt="#"></a>
                                            </td>
                                            <td>
                                                <h4><a href="/productDetails?id=<%=product.productId._id %>">
                                                        <%= product.productId.productName%>
                                                    </a></h4>
                                            </td>
                                            <td class="cart-product-price">&#8377;
                                                <%=product.productPrice %>
                                            </td>
                                            <td class="cart-product-quantity"><%=product.count %>
                                            </td>
                                            <!-- <td class="cart-product-stock">&#8377;<%=products.count %></td> -->
                                        </tr>
                                        <% }) %>
                                    </tbody>
                                </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- WISHLIST AREA START -->

    <%- include('../partials/userPartials/footer.ejs') %>